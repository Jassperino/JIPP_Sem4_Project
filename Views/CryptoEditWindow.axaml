<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        x:Class="JIPP_Projekt_Sem4.Views.CryptoEditWindow"
        Width="300" Height="300"
        Icon="/Assets/clicker.png"
        Title="Edytuj kryptowaluty">
    <StackPanel Margin="20" Spacing="5">
        <TextBlock Text="Bitcoin"/>
        <TextBox x:Name="BitcoinBox"/>
        <TextBlock Text="Ethereum"/>
        <TextBox x:Name="EthereumBox"/>
        <TextBlock Text="Tether"/>
        <TextBox x:Name="TetherBox"/>
        <TextBlock Text="ZCash"/>
        <TextBox x:Name="ZCashBox"/>
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Right" Spacing="5" Margin="0,10,0,0">
            <Button Content="Zapisz" Click="Save_Click"/>
            <Button Content="Anuluj" Click="Cancel_Click"/>
        </StackPanel>
    </StackPanel>
</Window>